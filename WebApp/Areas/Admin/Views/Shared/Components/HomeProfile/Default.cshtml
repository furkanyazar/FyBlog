@using Entities.Concrete

@model Admin

<div class="col-lg-4">
	<div class="widget-head-color-box navy-bg p-lg text-center">
		<div class="m-b-md">
			<h2 class="font-bold no-margins">
				@Model.User.UserFirstName @Model.User.UserLastName
			</h2>
			<small>@Model.AdminTitle</small>
		</div>
		<img src="@Model.User.UserImageUrl" class="rounded-circle circle-border m-b-md" alt="profile">
	</div>
	<div class="widget-text-box">
		<h4 class="media-heading">@Model.User.UserFirstName @Model.User.UserLastName</h4>
		<p>@Model.AdminAbout</p>
	</div>
</div>
